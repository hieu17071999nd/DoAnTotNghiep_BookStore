    <endEvent id="theEnd"></endEvent>
    <sequenceFlow id="flow1" sourceRef="start" targetRef="servicetask1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>

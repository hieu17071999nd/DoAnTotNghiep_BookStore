      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${(execution.getVariable('var_checkNumberProduct')) == false}]]></conditionExpression>
    </sequenceFlow>
    <intermediateCatchEvent id="timerintermediatecatchevent1" name="TimerCatchEvent">
      <timerEventDefinition>
        <timeDuration>PT5S</timeDuration>
      </timerEventDefinition>
    </intermediateCatchEvent>
    <sequenceFlow id="flow14" sourceRef="timerintermediatecatchevent1" targetRef="cancelOrderTask"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>

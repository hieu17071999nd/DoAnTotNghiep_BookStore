      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${(execution.getVariable('var_checkNumberProduct2')) == true}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="sendEmailCancel" name="Gửi email hủy đơn hàng" activiti:async="true" activiti:class="com.hieuvm.bookstore.task.SendEmailCancelServiceTask"></serviceTask>
    <sequenceFlow id="flow17" sourceRef="exclusivegateway2" targetRef="sendEmailCancel">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${(execution.getVariable('var_checkNumberProduct2')) == false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow18" sourceRef="sendEmailCancel" targetRef="theEnd"></sequenceFlow>
    <sequenceFlow id="flow19" sourceRef="substractInventoryTask" targetRef="mailtask2"></sequenceFlow>
    <sequenceFlow id="flow20" sourceRef="mailtask2" targetRef="assignedStaffTransportTask"></sequenceFlow>
    <userTask id="successfulDeliveryConfirmationTask" name="Giao hàng thành công"></userTask>
    <sequenceFlow id="flow21" sourceRef="assignedStaffTransportTask" targetRef="successfulDeliveryConfirmationTask"></sequenceFlow>
    <sequenceFlow id="flow22" sourceRef="successfulDeliveryConfirmationTask" targetRef="theEnd"></sequenceFlow>
    <userTask id="assignedStaffTransportTask" name="Chuyển hàng cho nhân viên giao hàng"></userTask>
    <sequenceFlow id="flow23" sourceRef="cancelOrderTask" targetRef="theEnd"></sequenceFlow>
    <serviceTask id="mailtask1" name="Gửi mail xác nhận" activiti:type="mail">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[hieumv99nd@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:string><![CDATA[hieumvnucek62@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Xác nhận duyệt đơn hàng]]></activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:string><![CDATA[<html>
            <body>
              Hello, Ban da duyet don hang thanh cong.
            </body>
</html>]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow24" sourceRef="approveOrderTask5" targetRef="mailtask1"></sequenceFlow>
    <sequenceFlow id="flow25" sourceRef="mailtask1" targetRef="exclusivegateway2"></sequenceFlow>
    <serviceTask id="mailtask2" name="Gửi mail đặt hàng thành công" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:string><![CDATA[hieumvnucek62@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[hieumv99nd@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Xác nhận đặt hàng đơn hàng thành công]]></activiti:string>
        </activiti:field>
        <activiti:field name="html">
          <activiti:string><![CDATA[<html>
            <body>
              Chúc mừng bạn đã đặt hàng thành công. Đơn hàng của bạn sẽ được giao cho đơn vị vận chuyển để giao hàng.
            </body>
</html>]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>

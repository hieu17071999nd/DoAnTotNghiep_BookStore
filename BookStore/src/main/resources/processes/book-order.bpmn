  <process id="bookOrder" name="bookOrder" isExecutable="true">
    <startEvent id="start" name="Start"></startEvent>
    <endEvent id="theEnd"></endEvent>
    <sequenceFlow id="flow1" sourceRef="start" targetRef="receiveOrderTask"></sequenceFlow>

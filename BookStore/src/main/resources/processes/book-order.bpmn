    <userTask id="approveOrderTask5" name="Duyệt đơn hàng" activiti:assignee="hieu" activiti:candidateUsers="admin" activiti:candidateGroups="staffs"></userTask>
    <sequenceFlow id="flow3" sourceRef="exclusivegateway1" targetRef="approveOrderTask5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${(execution.getVariable('var_checkNumberProduct')) == true}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="receiveOrderTask" name="Nhận đơn hàng" activiti:class="com.hieuvm.bookstore.task.ReceiveOrder">
      <documentation>Lưu trữ thông tin đơn hàng vào các bảng order</documentation>
    </serviceTask>
    <serviceTask id="substractInventoryTask" name="Trừ kho" activiti:async="true" activiti:class="com.hieuvm.bookstore.task.SubtractStockTask"></serviceTask>
    <serviceTask id="cancelOrderTask" name="Hủy đơn hàng" activiti:class="com.hieuvm.bookstore.task.CancelOrderTask"></serviceTask>

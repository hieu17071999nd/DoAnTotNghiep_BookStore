    <serviceTask id="servicetask2" name="Gửi email đặt đơn hàng thành công" activiti:class="com.hieuvm.bookstore.task.SendEmailServiceTask"></serviceTask>
    <serviceTask id="servicetask3" name="Gửi email hủy đơn hàng" activiti:class="com.hieuvm.bookstore.task.SendEmailServiceTask"></serviceTask>
